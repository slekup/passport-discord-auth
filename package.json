{
  "name": "passport-discord-auth",
  "version": "1.1.1",
  "description": "An updated passport authentication strategy for Discord.",
  "keywords": [
    "discord.js",
    "discord",
    "utils",
    "slekup",
    "nodejs",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slekup/passport-discord-auth"
  },
  "license": "CC-BY-NC-SA-4.0",
  "author": "Slekup",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "nodemon --watch src/** --ext ts,json --exec \"pnpm build\"",
    "commit": "commit",
    "dev": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src/**/*",
    "lint:fix": "eslint src/**/* --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "builder-validation": "^0.0.3",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-angular": "^19.2.2",
    "@commitlint/prompt-cli": "^19.2.2",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint-config-slekup": "^1.0.23",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.7.1"
}
